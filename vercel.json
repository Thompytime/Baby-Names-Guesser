{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "og-image.png",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/og-image.png",
      "headers": {
        "Content-Type": "image/png",
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/og-image.png"
    }
  ]
}
